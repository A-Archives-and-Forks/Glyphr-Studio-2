{
 "extends": [
  "eslint:recommended",
  "plugin:@typescript-eslint/eslint-recommended",
  "plugin:@typescript-eslint/recommended",
  "plugin:storybook/recommended"
 ],
 "plugins": [
  "prettier"
 ],
 "env": {
  "browser": true,
  "es6": true
 },
 "parserOptions": {
  "ecmaVersion": "latest",
  "sourceType": "module"
 },
 "rules": {
  "no-unused-vars": "warn"
 },
 "globals": {
  "log": "readonly"
 },
 "ignorePatterns": [
  "*.test.js",
  "dev/lib/*",
  "dev/samples/*",
  "dev/io/*",
  "REFACTOR_*",
  "opentype*"
 ]
}
