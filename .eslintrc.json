{
    "extends": "eslint:recommended",
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules":{
        "prettier/prettier": "error"
    },
    "ignorePatterns": [
        "*.test.js",
        "dev/lib/*",
        "dev/samples/*"
    ]
}
