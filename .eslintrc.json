{
    "extends": "eslint:recommended",
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules":{
        "no-unused-vars": "warn"
    },
    "globals": {
        "log": "off"
    },
    "ignorePatterns": [
        "*.test.js",
        "dev/lib/*",
        "dev/samples/*",
        "dev/io/*",
        "REFACTOR_*"
    ]
}
