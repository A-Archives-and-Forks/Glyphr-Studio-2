{
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/.vscode": true,
        "**/.git": true,
        "dev/lib/": true,
        "package.json": true,
        "package-lock.json": true,
        "README.md": true,
        "LICENSE*.txt": true,
        ".eslintrc.json": true,
        ".gitattributes": true,
        ".gitignore": true
    },
    "cSpell.enabled": false
}